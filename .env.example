# Contact Importer Environment Variables

# Default country code for phone number parsing
DEFAULT_COUNTRY_CODE=KE

# Default output format
DEFAULT_OUTPUT_FORMAT=vcf

# Logging level
LOG_LEVEL=INFO

# Output directory for generated contact files
OUTPUT_DIR=./output

# Maximum file size to process (in MB)
MAX_FILE_SIZE_MB=50

# Enable strict validation mode
STRICT_VALIDATION=false
